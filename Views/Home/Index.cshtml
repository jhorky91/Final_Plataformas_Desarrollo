@{
    ViewData["Title"] = "Home Page";
}
@model IEnumerable<Final_Plataformas_De_Desarrollo.Models.Producto>

<div class="text-center">
    <h1 class="display-4">Welcome</h1>
    <h4>@ViewData["Login"]</h4>
    <hr />

    <div class="container">
        <div class="row">
            @foreach (var prod in Model)
            {
                <div class="col-md-4">
                    <label>
                        @prod.nombre
                    </label>
                </div>
            }
        </div>
    </div>

</div>
